{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p routify rollup",
    "routify": "routify -D",
    "build": "routify -bD && rollup -c && routify export",
    "start": "sirv dist --single",
    "preview-build": "cd scripts/server && node index",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "export OUTPUT_TYPE=bundle || set OUTPUT_TYPE=BUNDLE && rollup -cw",
    "$env:bundle": "export OUTPUT_BUNDLE_BUILD=1 || set OUTPUT_BUNDLE_BUILD=1",
    "$env:dynamic": "export OUTPUT_DYNAMIC_BUILD=1 || set OUTPUT_DYNAMIC_BUILD=1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@sveltech/routify": "^1.1.1-2",
    "concurrently": "^5.0.2",
    "del": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "ppid-changed": "^1.0.1",
    "rollup": "^1.2.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy": "^3.2.1",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sirv-cli": "^1.0.0-next.2",
    "svelte": "3.19.2"
  },
  "dependencies": {}
}
