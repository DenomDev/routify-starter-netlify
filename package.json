{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "dev-prerender": "cross-env PRERENDER=true npm run dev",
    "build": "routify -bD && rollup -c",
    "serve": "spassr --serve-spa",
    "export": "routify export",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "routify": "routify -D"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@sveltech/routify": "^1.5.0-beta.38",
    "concurrently": "^5.0.2",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "ppid-changed": "^1.0.1",
    "rollup": "^1.2.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy": "^3.2.1",
    "rollup-plugin-livereload": "^1.1.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "spassr": "^1.0.1",
    "svelte": "3.19.2"
  },
  "dependencies": {}
}
